{
  "name": "electron-react",
  "version": "1.0.3",
  "main": "main.js",
  "scripts": {
    "start-electron": "cross-env NODE_ENV=development ./node_modules/.bin/electron .",
    "start": "cross-env NODE_ENV=development webpack serve --config webpack.config.js",
    "build-render": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build-electron": "./node_modules/.bin/electron-builder build -mwl",
    "build": "npm run build-render && npm run build-electron"
  },
  "build": {
    "appId": "com.zhifou.electron",
    "productName": "electron-react",
    "copyright": "Copyright © 2020",
    "asar": true,
    "directories": {
      "buildResources": "build",
      "output": "release"
    },
    "files": [
      "dist/",
      "main.js"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "dmg": {
      "title": "我是electron",
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 340,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.1",
    "electron": "^12.0.4",
    "electron-builder": "^22.10.5",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^5.3.1",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "postcss-loader": "^3.0.0",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.0",
    "webpack": "^5.33.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "babel-plugin-import": "^1.13.3",
    "history": "^5.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  }
}
