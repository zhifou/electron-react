{
  "name": "electron-react",
  "version": "1.0.2",
  "main": "main.js",
  "scripts": {
    "start-electron": "./node_modules/.bin/cross-env NODE_ENV=development ./node_modules/.bin/electron .",
    "start": "./node_modules/.bin/cross-env NODE_ENV=development ./node_modules/.bin/webpack-dev-server --config webpack.config.js",
    "build-render": "./node_modules/.bin/webpack --config webpack.config.js",
    "build-electron": "./node_modules/.bin/electron-builder build -mwl",
    "build": "npm run build-render && npm run build-electron"
  },
  "build": {
    "appId": "com.zhifou.electron",
    "productName": "electron-react",
    "copyright": "Copyright Â© 2020",
    "files": [
      "dist/",
      "main.js"
    ],
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 340,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "directories": {
      "buildResources": "assets",
      "output": "release"
    }
  },
  "devDependencies": {
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.1",
    "electron": "^9.2.0",
    "electron-builder": "^22.8.0",
    "html-webpack-plugin": "^4.3.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^9.0.3",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.2",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "file-loader": "^6.0.0",
    "url-loader": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "autoprefixer": "^9.8.6"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "antd": "^4.5.4",
    "babel-plugin-import": "^1.13.0"
  }
}
